{
    "name":"contao-bootstrap/navbar",
    "description":"Bootstrap navbar component for Contao CMS",
    "keywords":["bootstrap", "components", "contao", "navbar"],
    "type":"contao-bundle",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"David Molineus",
            "email":"mail@netzmacht.de",
            "homepage":"https://netzmacht.de",
            "role":"Developer"
        }
    ],
    "support":{
        "email":"mail@netzmacht.de",
        "issues":"https://github.com/contao-bootstrap/navbar/issues",
        "wiki":"http://contao-bootstrap.netzmacht.de",
        "source":"https://github.com/contao-bootstrap/navbar"
    },
    "require":{
        "php":">=5.6.0",
        "contao-community-alliance/composer-plugin": "~3.0",
        "contao/core-bundle":"^4.3",
        "contao-bootstrap/core": "^2.0",
        "menatwork/contao-multicolumnwizard":"^3.3.4",
        "netzmacht/html": "~1.2",
        "contao-community-alliance/meta-palettes":"^1.10"
    },
    "require-dev": {
        "netzmacht/contao-build-tools": "~1.1",
        "contao/manager-plugin": "^2.0"
    },
    "conflict": {
        "netzmacht/contao-bootstrap": "*",
    },
    "autoload": {
        "psr-4": { "ContaoBootstrap\\Navbar\\": "src/" }
    },
    "extra":{
        "contao": {
            "transifex": {
                "project": "contao-bootstrap",
                "prefix": "navbar-",
                "languages_cto": "src/Resources/contao/languages",
                "languages_tx": ".tx"
            }
        },
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-develop": "2.1.x-dev"
        },
        "contao-manager-plugin": "ContaoBootstrap\\Navbar\\ContaoManager\\Plugin"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
